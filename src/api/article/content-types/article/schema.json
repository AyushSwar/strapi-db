{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "excerpt": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "publishDate": {
      "type": "date"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Astrology",
        "Tarot",
        "Love & Relationships",
        "Crystals & Healing",
        "Moon Phases",
        "Numerology"
      ]
    },
    "readTime": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "featured": {
      "type": "boolean"
    },
    "content": {
      "type": "richtext"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
